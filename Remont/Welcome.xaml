<Window x:Class="Remont.Welcome"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Remont"
        mc:Ignorable="d"
        Title="Welcome" Height="450" Width="800">
    <Grid x:Name="bt7">
        <Button x:Name="bt2" Content="Информация о заявках" HorizontalAlignment="Left" Margin="28,34,0,0" VerticalAlignment="Top" Height="34" Width="143" Click="bt2_Click" Visibility="Hidden"/>
        <Button x:Name="bt5" Content="Отчёт" HorizontalAlignment="Left" Margin="28,131,0,0" VerticalAlignment="Top" Height="34" Click="bt5_Click" Width="143" Visibility="Hidden"/>
