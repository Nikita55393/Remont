        <Button x:Name="bt3" Content="Отследить сататус заявки" HorizontalAlignment="Left" Margin="28,83,0,0" VerticalAlignment="Top" Height="34" Click="bt3_Click" Visibility="Hidden"/>
        <Button x:Name="bt6" Content="Зафиксировать инф. о запасных запчастях" HorizontalAlignment="Left" Margin="28,131,0,0" VerticalAlignment="Top" Height="34" Width="143" Click="bt6_Click" Visibility="Hidden"/>
        <Button Content="Назад" HorizontalAlignment="Left" Margin="692,348,0,0" VerticalAlignment="Top" Height="34" Width="76" Click="Button_Click"/>

    </Grid>
